{
  "name": "@t8/react-router",
  "version": "1.1.4",
  "description": "Concise router for React apps",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx npm-run-all clean compile",
    "clean": "node -e \"require('node:fs').rmSync('dist', { force: true, recursive: true });\"",
    "compile": "npx esbuild index.ts --bundle --outdir=dist --platform=neutral --external:react",
    "demo": "npx @t8/serve 3000 * tests/route_matching -b src/index.tsx index.js",
    "prepublishOnly": "npm run build",
    "preversion": "npx npm-run-all typecheck shape build test",
    "shape": "npx codeshape",
    "test": "npx playwright test --project=chromium",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "react router",
    "router",
    "route",
    "spa",
    "history",
    "link"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/t8js/react-router.git"
  },
  "homepage": "https://t8.js.org/react-router",
  "author": "axtk",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@t8/serve": "^0.1.30",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.9",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.3",
    "url-shape": "^1.2.8",
    "zod": "^4.1.12"
  },
  "dependencies": {
    "@t8/router": "^1.1.2"
  }
}
