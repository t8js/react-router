{
  "name": "@t8/react-router",
  "version": "1.3.6",
  "description": "Concise router for React apps",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "demo": "npx @t8/serve tests/routing --spa -b src/index.tsx",
    "demo-route-state": "npx @t8/serve tests/typed_route_state --spa -b src/index.tsx",
    "demo-view-transitions": "npx @t8/serve tests/view_transitions --spa -b src/index.tsx",
    "preversion": "npx npm-run-all shape test",
    "shape": "npx codeshape",
    "test": "npx playwright test --project=chromium"
  },
  "keywords": [
    "react",
    "react router",
    "router",
    "route",
    "spa",
    "history",
    "link"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/t8js/react-router.git"
  },
  "homepage": "https://t8.js.org/react-router",
  "author": "axtk",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@t8/serve": "^0.2.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react-dom": "^19.2.3",
    "url-shape": "^1.3.12",
    "zod": "^4.1.13"
  },
  "dependencies": {
    "@t8/router": "^1.3.6"
  }
}
